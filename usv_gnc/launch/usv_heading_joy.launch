<launch>
	<include file="$(find uri_usv)/launch/usv_sensor_on.launch"/>
	<node name="auto_pwm" pkg="uri_usv" type="auto_pwm" output="screen"> 
	</node>
	<node name="heading_PID" pkg="uri_usv" type="heading_PID" output="screen">
		<param name = "K_p"  value ="40.0"/>
		<param name = "K_i"   value ="0.01"/>
		<param name = "K_d"   value ="20.0"/>
		<param name = "K_g"   value ="0.001"/>
		<param name = "Total_thrust" value ="1.6"/>
	</node>
</launch>
